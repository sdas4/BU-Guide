%h1 All Users

%table#users
  %thead
    %tr
      %th Username
      %th Password
      %th EmailID
  %tbody
    - @users.each do |user|
      %tr
        %td= user.username
        %td= user.password
        %td= user.emailID

= link_to '<button type="button">Back to Dashboard</button>'.html_safe, guide_dashboard_path